#include <iostream>

int main() {
  int dinheiro_gasto, anos;
  float preco;

  std::cout << "Digite o preço por ano!\n";
  std::cin >> preco;

  std::cout << "Digite a quantidade de anos!\n";
  std::cin >> anos;

  // Calcula o dinheiro gasto multiplicando o preço pelos anos
  dinheiro_gasto = preco * anos; 

  std::cout << "Seu dinheiro gasto é: " << dinheiro_gasto << std::endl;

  return 0;
}
